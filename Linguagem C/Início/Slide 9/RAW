(Pontos colineares)
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
typedef struct{
    int x,y;
}Ponto;
int distanciaPontos(Ponto a, Ponto b){
    int d;
    if(a.x>b.y)
        d=a.x-b.y;
    else
        d=b.y-a.x;
    return d;
}
int main(){
    Ponto c1,c2;
    int distancia;
    printf("Digite a coordenada: ");scanf("%d",&c1.x);
    printf("Digite a coordenada: ");scanf("%d",&c2.y);
    distancia=distanciaPontos(c1,c2);
    printf("A distancia entre pontos e: %d",distancia);
    return 0;
}

(Pontos n√£o coplanares)
#include <stdio.h>
#include <stdlib.h>
#include <math.h>
typedef struct{
    int x,y;
}Ponto;
float distanciaPontos(Ponto a, Ponto b){
    float d;
    d=(float)sqrt(pow(a.x,2)+pow(b.y,2));
    return d;
}
int main(){
    Ponto c1,c2;
    float distancia;
    printf("Digite a coordenada: ");scanf("%d",&c1.x);
    printf("Digite a coordenada: ");scanf("%d",&c2.y);
    distancia=distanciaPontos(c1,c2);
    printf("A distancia entre pontos e: %.3f",distancia);
    return 0;
}

	6-


#include <stdio.h>
#include <stdlib.h>
#include <math.h>
typedef struct{
    int x,y;
}Ponto;
int distanciaPontos(Ponto a, Ponto b){
    int d;
    if(abs(a.x)>abs(b.y))
        d=b.y;
    else
        d=a.x;
    return d;
}
int main(){
    Ponto c1,c2;
    int distancia;
    printf("Digite a coordenada: ");scanf("%d",&c1.x);
    printf("Digite a coordenada: ");scanf("%d",&c2.y);
    distancia=distanciaPontos(c1,c2);
    printf("O ponto mais proximo da origem e: %d",distancia);
    return 0;
}

