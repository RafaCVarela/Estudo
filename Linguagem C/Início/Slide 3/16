#include <stdio.h>
#include <stdlib.h>

int main()
{
    int id;
    
    printf(" Descubra sua categoria em natacao, digite sua idade: ");
    scanf("%d", &id);
    
    switch(id){
        case 1: case 2: case 3: case 4: 
        printf(" Idade invalida, pois nao participa de nenhuma categoria.\n");
        break;
        
        case 5: case 6: case 7: 
        printf(" Infantil A\n");
        break;
        
        case 8: case 9: case 10: 
        printf(" Infantil B\n");
        break;
        
        case 11: case 12: case 13: 
        printf(" Juvenil A\n");
        break;
        
        case 14: case 15: case 16: case 17: 
        printf(" Juvenil B\n");
        break;
        
        case 18: case 19: case 20: case 21: case 22: case 23: case 24: case 25: case 26: case 27: case 28: case 29:
case 30: 
        printf(" Adulto\n");
        break;
        
        default: 
        printf(" Seniors\n");
        
    }
    
    return 0;
    
}
