def codigo(x):

    if x == 1:
        v = 4.00
    elif x == 2:
        v = 4.50
    elif x == 3:
        v = 5.00
    elif x == 4:
        v = 2.00
    elif x == 5:
        v = 1.50

    return v

a, b = input().split()

a = int(a)
b = int(b)

soma = codigo(a) * b

print(f'Total: R$ {soma:.2f}')

