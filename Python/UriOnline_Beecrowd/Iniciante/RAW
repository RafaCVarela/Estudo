while True:

    soma = 0
    
    X = int(input())

    if X==0:
        break
    
    if X%2!=0:
        
        X += 1
        indicePar = X//2

    else:

        indicePar = X//2

    for i in range(5):

        soma += 2*(i + indicePar)

    print(soma)


