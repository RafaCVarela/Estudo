while(True):

    M, N = map(int, input().split())
    
    if (M<= 0 or N<= 0):
        break
    
    else:
        if (M > N):

            soma  = 0

            for x in range(N, M + 1):

                soma = x + soma
                print(x, end=" ")
            print(f"Sum={soma}")

        else:
            soma  = 0

            for x in range(M, N + 1):

                soma = x + soma
                print(x, end=" ")
            print(f"Sum={soma}")

