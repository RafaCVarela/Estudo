n = int(input())

numCoelhos = 0
numRatos = 0
numSapos = 0

for i in range(n):
    num, tipo = input().split()
    if tipo == 'C':
        numCoelhos += int(num)
    elif tipo == 'S':
        numSapos += int(num)
    else:
        numRatos += int(num)

total = numCoelhos + numRatos + numSapos

pC = float(numCoelhos / total) * 100.0
pS = float(numSapos / total) * 100.0
pR = float(numRatos / total) * 100.0

print(f'Total: {total} cobaias\nTotal de coelhos: {numCoelhos}\nTotal de ratos: {numRatos}\nTotal de sapos: {numSapos}')
print(f'Percentual de coelhos: {pC:.2f} %\nPercentual de ratos: {pR:.2f} %\nPercentual de sapos: {pS:.2f} %')

