i = 0
j = 1

while i <= 2:
    for n in range(3):
        di = round(i - int(i), 1)
        dj = round(j - int(j), 1)
        
        if di != 0.0 and dj != 0.0:
            print(f'I={i:.1f} J={j:.1f}')
        elif di == 0.0 and dj != 0.0:
            print(f'I={i} J={j:.1f}')
        elif di != 0.0 and dj == 0.0:
            print(f'I={i:.1f} J={j}')
        elif di == 0.0 and dj == 0.0:
            print(f'I={int(i)} J={int(j)}')
        j += 1
    
    i = round(i + 0.2, 1)
    j = round(1 + i, 1)

	60-

def somaImpares(x, y):
    soma = 0
    i = x + 1

    while i < y:
        if i % 2 != 0:
            soma += i

        i += 1

    return soma


n = int(input())

for i in range(n):
    a, b = map(int, input().split())

    if a > b:
        resp = somaImpares(b, a)
        print(f'{resp}')
    elif a < b:
        resp = somaImpares(a, b)
        print(f'{resp}')
    else:
        print(0)

