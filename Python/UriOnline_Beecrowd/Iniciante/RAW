    x = int(input())
    if x > 0:
        intPositivos += 1
        if x % 2 == 0:
            pares += 1
        else:
            impares += 1
    elif x < 0:
        intNegativos += 1

        if x % 2 == 0:
            pares += 1
        else:
            impares += 1
    else:
        pares += 1


