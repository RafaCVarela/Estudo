N = int(input())

for i in range(N):

    soma = 0
    
    X, Y = map(int, input().split())

    if X%2==0:
        indiceImpar = X//2
    else:
        indiceImpar = (X-1)//2

    for j in range(Y):

        soma += 2*(j+indiceImpar) + 1

    print(soma)        


