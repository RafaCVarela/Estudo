t0, tf = map(int, input().split())

if t0 == tf:
    print('O JOGO DUROU 24 HORA(S)')
elif t0 > tf:
    T = (24 - t0) + tf
    print(f'O JOGO DUROU {T} HORA(S)')
else:
    T = tf - t0
    print(f'O JOGO DUROU {T} HORA(S)')

	33-

def h_m(t):
    minutos = int(t % 60)
    horas = int((t - (t % 60)) / 60)

    print(f'O JOGO DUROU {horas} HORA(S) E {minutos} MINUTO(S)')

h0, m0, hf, mf = map(int, input().split())

x = 60 * h0 + m0
y = 60 * hf + mf

if x == y:
    print('O JOGO DUROU 24 HORA(S) E 0 MINUTO(S)')
elif x > y:
    T = (24 * 60 - x) + y
    h_m(T)
else:
    T = y - x
    h_m(T)

