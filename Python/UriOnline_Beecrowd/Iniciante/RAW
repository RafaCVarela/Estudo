def somaImpares(x, y):
    n = 1
    soma = 0

    while (n + x) < y:
        if (n + x) % 2 != 0:
            soma += (n + x)
        n += 1
    print(soma)


a = int(input())
b = int(input())

if a < b:
    somaImpares(a, b)
elif a > b:
    somaImpares(b, a)
else:
    print(0)

