numeroGrenais = 1
vitoriaInter = 0
vitoriaGremio = 0
empate = 0

while True:
    
    golInter, golGremio = map(int, input().split())

    if golInter > golGremio:
        vitoriaInter += 1
    elif golInter < golGremio:
        vitoriaGremio += 1
    else:
        empate += 1

    print('Novo grenal (1-sim 2-nao)')
    flag = int(input())

    if flag == 2:
        print(f'{numeroGrenais} grenais\nInter:{vitoriaInter}')
        print(f'Gremio:{vitoriaGremio}\nEmpates:{empate}')

        if vitoriaInter > vitoriaGremio:
            print('Inter venceu mais')
        elif vitoriaInter < vitoriaGremio:
            print('Gremio venceu mais')
        else:
            print('Nao houve vencedor')
        
        break
    else:
        numeroGrenais += 1


