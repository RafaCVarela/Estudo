def imposto(x):
    if x >= 0.0 and x <= 2000.0:
        return 0 
    elif x >= 2000.01 and x <= 3000.0:
        valor = imposto(2000) + (x - 2000) * 0.08
        return valor
    elif x >= 3000.01 and x <= 4500.0:
        valor = imposto(3000) + (x - 3000) * 0.18
        return valor
    elif x > 4500.0:
        valor = imposto(4500) + (x - 4500) * 0.28
        return valor

x = float(input())

if x >= 0.0 and x <= 2000.0:
    print('Isento')
elif x >= 2000.01 and x <= 3000.0:
    valor = (x - 2000) * 0.08
    print(f'R$ {valor:.2f}')
elif x >= 3000.01 and x <= 4500.0:
    valor = imposto(3000) + (x - 3000) * 0.18
    print(f'R$ {valor:.2f}')
elif x > 4500.0:
    valor = imposto(4500) + (x - 4500) * 0.28
    print(f'R$ {valor:.2f}')

