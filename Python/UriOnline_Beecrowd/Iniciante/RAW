indice = int(input(''))

alcool = 0
gasolina = 0
diesel = 0

while True:

    match indice:

        case 1:
            alcool += 1
        case 2:
            gasolina += 1
        case 3:
            diesel += 1
        case 4: 
            break
        case _: 
            pass

    indice = int(input(''))

print(f'MUITO OBRIGADO\nAlcool: {alcool}\nGasolina: {gasolina}\nDiesel: {diesel}')


