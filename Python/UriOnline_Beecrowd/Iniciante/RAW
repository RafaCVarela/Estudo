def comparador(p, q):
    if p > q:
        return True
    else:
        return False
    
def ordenador(a, b, c):
    if comparador(a, b):
        if comparador(a, c):
            if comparador(b, c):
                print(f'{c}\n{b}\n{a}\n')
            else:
                print(f'{b}\n{c}\n{a}\n')
        else:
            print(f'{b}\n{a}\n{c}\n')
    else:
        if comparador(a, c):
            print(f'{c}\n{a}\n{b}\n')
        else:
            if comparador(b, c):
                print(f'{a}\n{c}\n{b}\n')
            else:
                print(f'{a}\n{b}\n{c}\n')

x, y, z = map(int, input().split())

ordenador(x, y, z)

print(f'{x}\n{y}\n{z}')

	29-

def desi_tri(x, y, z):
    if x >= y + z or x <= abs(y - z):
        return False
    elif y >= x + z or y <= abs(x - z):
        return False
    elif z >= x + y or z <= abs(x - y):
        return False
    else:
        return True

a, b, c = map(float, input().split())

if desi_tri(a, b, c):
    P = a + b + c
    print(f'Perimetro = {P:.1f}')
else:
    S = (a + b) * c / 2
    print(f'Area = {S:.1f}')

