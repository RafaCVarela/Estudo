	31-

def lados(x, y, z):
    if x == y:
        if x == z:
            print('TRIANGULO EQUILATERO')
        else:
            print('TRIANGULO ISOSCELES')
    elif y == z:
        print('TRIANGULO ISOSCELES')

def natu_tri(a, b, c):
    if a**2 == b**2 +  c**2:
        print('TRIANGULO RETANGULO')
    elif a**2 > b**2 + c**2:
        print('TRIANGULO OBTUSANGULO')
    elif a**2 < b**2 + c**2:
        print('TRIANGULO ACUTANGULO')
    lados(a, b, c)

def desi_tri(x, y, z):
    if x >= y + z:
        print('NAO FORMA TRIANGULO')
    else:
        natu_tri(x, y, z)

def comparador(p, q):
    if p >= q:
        return True
    else:
        return False

def ordenador(a, b, c):
    if comparador(a, b):
        if comparador(a, c):
            if comparador(b, c):
                desi_tri(a, b, c)
            else:
                desi_tri(a, c, b)
        else:
            desi_tri(c, a, b)
    else:
        if comparador(a, c):
            desi_tri(b, a, c)
        else:
            if comparador(b, c):
                desi_tri(b, c, a)
            else:
                desi_tri(c, b, a)

x, y, z = map(float, input().split())

ordenador(x, y, z)

