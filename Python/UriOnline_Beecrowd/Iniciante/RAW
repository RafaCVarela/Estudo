import math
a, b, c = map(float, input().split())

delta = math.pow(b, 2) - 4.0 * a * c

if delta < 0.0 or a == 0.0:
    print('Impossivel calcular')

else:
    x1 = (-b + delta**(1 / 2)) / (2.0 * a)
    x2 = (-b - delta**(1 / 2)) / (2.0 * a)
    print(f'R1 = {x1:.5f}\nR2 = {x2:.5f}')

