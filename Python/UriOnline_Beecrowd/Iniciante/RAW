	41-

delimiter = ' : '

strDia0, data0 = input().split()
h0, min0, seg0 = map(int, input().split(delimiter))

strDiaf, dataf = input().split()
hf, minf, segf = map(int, input().split(delimiter))

data0 = int(data0)
dataf = int(dataf)

if data0 == dataf:
    dataT = int(dataf - data0)
    hT = hf - h0
    minT = minf - min0
    segT = segf - seg0

    print(f'{dataT} dia(s)\n{hT} hora(s)\n{minT} minuto(s)\n{segT} segundo(s)')

else:
    dataT = dataf - data0 - 1
    
    somaSeg0 = h0 * 3600 + min0 * 60 + seg0
    seg24H = 24 * 3600
    d0 = seg24H - somaSeg0

    somaSegf = hf * 3600 + minf * 60 + segf

    somaSegTotal = d0 + somaSegf

    segundos = int(somaSegTotal % 60)
    x = (somaSegTotal - segundos) / 60

    minutos = int(x % 60)
    y = (x - minutos) / 60

    horas = int(y % 24)
    z = (y - horas) / 24

    data = int(dataT + z)

    print(f'{data} dia(s)\n{horas} hora(s)\n{minutos} minuto(s)\n{segundos} segundo(s)')

	42-

def soma_positivos(x, soma):
    if x > 0:
        soma += x
        return soma
    else:
        return soma


def cont_positivos(x, cont):
    if x > 0:
        cont += 1
        return cont
    else:
        return cont


cont = 0
soma = 0

a = float(input())
cont = cont_positivos(a, cont)
soma = soma_positivos(a, soma)
b = float(input())
cont = cont_positivos(b, cont)
soma = soma_positivos(b, soma)
c = float(input())
cont = cont_positivos(c, cont)
soma = soma_positivos(c, soma)
d = float(input())
cont = cont_positivos(d, cont)
soma = soma_positivos(d, soma)
e = float(input())
cont = cont_positivos(e, cont)
soma = soma_positivos(e, soma)
f = float(input())
cont = cont_positivos(f, cont)
soma = soma_positivos(f, soma)

cont = int(cont)
media = float(soma / cont)

print(f'{cont} valores positivos\n{media:.1f}')

