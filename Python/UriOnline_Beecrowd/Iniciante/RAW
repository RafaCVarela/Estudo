def reajuste(salario, i):
    nsalario = salario * (1 + i)

    print(f'Novo salario: {nsalario:.2f}\nReajuste ganho: {(salario * i):.2f}\nEm percentual: {int(i * 100)} %')

x = float(input())

if x >= 0 and x <= 400.0:
    reajuste(x, 0.15)

elif x >= 400.01 and x <= 800.0:
    reajuste(x, 0.12)

elif x >= 800.01 and x <= 1200.0:
    reajuste(x, 0.10)

elif x >= 1200.01 and x <= 2000.0:
    reajuste(x, 0.07)

elif x > 2000.0:
    reajuste(x, 0.04)

