# Usando funções, imprime uma caixa

def imprima1():
    print('+', '-' * 4, '+', '-' * 4, '+')

def imprima2():
    print('|', ' ' * 4, '|', ' ' * 4, '|')
    print('|', ' ' * 4, '|', ' ' * 4, '|')
    print('|', ' ' * 4, '|', ' ' * 4, '|')
    print('|', ' ' * 4, '|', ' ' * 4, '|')

def imprimetudo():
    imprima1()
    imprima2()
    imprima1()
    imprima2()
    imprima1()

imprimetudo()
